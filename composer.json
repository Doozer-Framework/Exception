{
  "name": "doozr-framework/exception",
  "license": "MIT",
  "description": " Exception trait enriching classes with parser, interpreter and compiler.",
  "type": "library",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Lead"
    }
  ],
  "keywords": [
    "exception",
    "expression",
    "language",
    "doozer-framework",
    "clickalicious"
  ],
  "homepage": "https://doozer.readme.io/",
  "support": {
    "issues": "https://github.com/Doozer-Framework/Exception/issues",
    "wiki": "https://github.com/Doozer-Framework/Exception/wiki",
    "tests": "https://doozer-framework.github.io/Exception"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "symfony/var-dumper": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Doozer\\Exception\\": "src/Exception/"
    },
    "exclude-from-classmap": [
      "tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Doozer\\Exception\\Tests": "tests/Exception/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "use-include-path": false,
    "preferred-install": "dist"
  }
}
